<?xml version="1.0"?>
<robot name="linear_block_robot">

  <!-- Link Definitions -->
  <link name="block1">
    <visual>
      <origin xyz="0. 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="1 0.35 3"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0. 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="1 0.35 3"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/> 
      <origin xyz="0. 0 0"/> 
      <inertia ixx="0.76" iyy="0.83" izz="0.094"/> 
    </inertial>
  </link>

  <link name="block2">
    <visual>
      <origin xyz="0.5 0 0" rpy="0 0 0"/> 
