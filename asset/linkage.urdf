  <!-- Joint Definitions -->
  <joint name="joint1" type="revolute">
    <parent link="block1"/>
    <child link="block2"/>
    <origin xyz="0.5 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000" lower="-.57" upper=".57" velocity="50"/>
    <dynamics damping="0.001" friction="0.001"/>
  </joint>

  <joint name="joint2" type="revolute">
